{
  "manifest_version": 3,
  "name": "Sora Auto Save",
  "version": "1.2.0",
  "description": "Automatically save images and prompts from Sora ChatGPT library with policy violation content support",
  "permissions": [
    "storage",
    "downloads",
    "activeTab"
  ],
  "host_permissions": [
    "https://sora.chatgpt.com/*"
  ],
  "content_scripts": [
    {
      "matches": ["https://sora.chatgpt.com/*"],
      "js": ["content.js"],
      "run_at": "document_end"
    }
  ],
  "action": {
    "default_popup": "popup.html",
    "default_title": "Sora Auto Save"
  },
  "web_accessible_resources": [
    {
      "resources": ["data.json"],
      "matches": ["https://sora.chatgpt.com/*"]
    }
  ],
  "default_locale": "en"
} 